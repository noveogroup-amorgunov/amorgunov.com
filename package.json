{
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development run-p -l start:*",
    "start:webpack": "webpack --mode development --watch --config configs/webpack.config.js",
    "start:11ty": "eleventy --input=src --config=configs/11ty.config.js --serve --quiet",
    "build": "NODE_ENV=production npm-run-all -l build:*",
    "build:clean": "rimraf dist",
    "build:webpack": "webpack --mode production --config configs/webpack.config.js",
    "build:11ty": "eleventy --input=src --config=configs/11ty.config.js",
    "analyze:js": "source-map-explorer dist/assets/build/**/*.js dist/assets/build/**/*.js.map",
    "create-post": "node .scripts/newPost",
    "update-post-likes": "node .scripts/updatePostLikes",
    "optimize-image": "node .scripts/optimizeImage",
    "format": "prettier --write src/**/*.{ts,js,css,json}",
    "lint": "npm-run-all lint:*",
    "lint:css": "stylelint src/assets/stylesheets/*.css",
    "lint:js": "eslint --ext .js,.ts src/assets/javascript"
  },
